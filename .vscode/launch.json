{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--enable_cameras"
            ],
            "console": "integratedTerminal",
            "env": {
                "DISPLAY": ":2"
            }
        }
    ]
}