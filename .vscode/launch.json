{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--enable_cameras",
                "--scene_folder",
                "/data/isaac_scenes_v1/",
                "--episode_path",
                "episodes/test.json"
            ],
            "console": "integratedTerminal",
            "env": {
                "DISPLAY": ":1"
            }
        }
    ]
}